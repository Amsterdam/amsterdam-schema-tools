{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.data.amsterdam.nl/v0.1/milieuzones",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "dataset",
    "type"
  ],
  "properties": {
    "id": {
      "$ref": "https://schemas.data.amsterdam.nl/v0.1#/definitions/id"
    },
    "type": {
      "type": "string"
    },
    "dataset": {
      "$ref": "https://schemas.data.amsterdam.nl/v0.1#/definitions/dataset"
    },
    "verkeerstype": {
      "description": "Verkeerstype",
      "type": "string",
      "enum": [
        "touringcar",
        "brom- en snorfiets",
        "taxi",
        "bestel",
        "vracht"
      ]
    },
    "datumVanaf": {
      "description": "Datum vanaf",
      "type": "string",
      "format": "date"
    },
    "geometry": {
      "description": "Geometrie",
      "type": "object"
    }
  }
}