{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://ams-schema.glitch.me/dataset/verhardingen/objecten@v0.1",
  "type": "object",
  "required": [
    "id",
    "dataset",
    "class"
  ],
  "properties": {
    "id": {
      "$ref": "https://ams-schema.glitch.me/schema@v0.1#/definitions/id",
      "description": "Object-ID"
    },
    "class": {
      "$ref": "https://ams-schema.glitch.me/schema@v0.1#/definitions/class"
    },
    "dataset": {
      "$ref": "https://ams-schema.glitch.me/schema@v0.1#/definitions/dataset"
    },
    "aanleghoogte": {
      "type": "number",
      "description": "Aanleghoogte (in meters)"
    },
    "aanofvrijliggendom": {
      "type": "string",
      "description": "aanofvrijliggendom"
    },
    "aanschafprijs": {
      "type": "number",
      "description": "Aanlegprijs (euro)"
    },
    "aantaldocumenten": {
      "type": "number",
      "description": "Aantal documenten"
    },
    "kwaliteitsniveauactueel": {
      "type": "string",
      "description": "Kwaliteitsniveau"
    },
    "afmeting": {
      "type": "string",
      "description": "Afmeting"
    },
    "begingarantieperiode": {
      "type": "string",
      "description": "Afmeting",
      "format": "date-time"
    },
    "typebeheerder": {
      "type": "string",
      "description": "Type beheerder"
    },
    "typebeheerderplus": {
      "type": "string",
      "description": "Type beheerder plus"
    },
    "beheergebied": {
      "type": "string",
      "description": "Beheergebied"
    },
    "beheervak": {
      "type": "string",
      "description": "Beheervak"
    },
    "belasting": {
      "type": "string",
      "description": "Belasting"
    },
    "bergendvermogen": {
      "type": "number",
      "description": "Bergend vermogen"
    },
    "breedte": {
      "type": "number",
      "description": "Breedte"
    },
    "constructie": {
      "type": "string",
      "description": "Constructie"
    },
    "aantaldeklagen": {
      "type": "number",
      "description": "Aantal deklagen"
    },
    "dikteconstructie": {
      "type": "number",
      "description": "Dikte constructie"
    },
    "formaat": {
      "type": "string",
      "description": "formaat"
    },
    "gebruiksfunctie": {
      "type": "string",
      "description": "Gebruiksfunctie"
    },
    "geluidsreducerend": {
      "type": "number",
      "description": "Geluidsreducerend"
    },
    "geometrie": {
      "$ref": "https://geojson.org/schema/MultiPolygon.json"
    },
    "kwaliteitsniveaugewenst": {
      "type": "string",
      "description": "Gewenst kwaliteitsniveau"
    },
    "grondsoort": {
      "type": "string",
      "description": "Grondsoort"
    },
    "grondsoortplus": {
      "type": "string",
      "description": "Grondsoort plus"
    },
    "buurt": {
      "ams.$ref.class": "https://ams-schema.glitch.me/dataset/gebieden/buurten",
      "$ref": "https://ams-schema.glitch.me/schema@v0.1#/definitions/uri",
      "description": "Buurt waarin object ligt"
    },
    "buurtcode": {
      "type": "string",
      "description": "Buurtcode"
    },
    "buurtnaam": {
      "type": "string",
      "description": "Buurtnaam"
    },
    "wijk": {
      "ams.$ref.class": "https://ams-schema.glitch.me/dataset/gebieden/wijken",
      "$ref": "https://ams-schema.glitch.me/schema@v0.1#/definitions/uri",
      "description": "Wijk waarin object ligt"
    },
    "wijkcode": {
      "type": "string",
      "description": "Wijkcode"
    },
    "wijknaam": {
      "type": "string",
      "description": "Wijknaam"
    },
    "stadsdeel": {
      "ams.$ref.class": "https://ams-schema.glitch.me/dataset/gebieden/stadsdelen",
      "$ref": "https://ams-schema.glitch.me/schema@v0.1#/definitions/uri",
      "description": "Stadsdeel waarin object ligt"
    },
    "stadsdeelcode": {
      "type": "string",
      "description": "Stadsdeelcode"
    },
    "stadsdeelnaam": {
      "type": "string",
      "description": "Stadsdeelnaam"
    }
  }
}
