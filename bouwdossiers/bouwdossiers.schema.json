{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.data.amsterdam.nl/v0.1/bouwdossiers",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "stadsdeel": {
      "$id": "https://schemas.data.amsterdam.nl/v0.1/gebieden#stadsdeel",
      "$ref": "https://schemas.data.amsterdam.nl/v0.1/datapunt#/definitions/uri"
    },
    "titel": {
      "type": "string"
    },
    "datering": {
      "$ref": "https://schemas.data.amsterdam.nl/v0.1/datapunt#/definitions/YearOptionalMonthOptionalDay"
    },
    "dossiertype": {
      "type": "string"
    },
    "openbaar": {
      "type": "boolean"
    },
    "dossiernummer": {
      "type": "string"
    },
    "adressen": {
      "type": "array",
      "items": {
        "$id": "https://schemas.data.amsterdam.nl/v0.1/bag#verblijfsobject",
        "$ref": "https://schemas.data.amsterdam.nl/v0.1/datapunt#/definitions/uri"
      }
    },
    "subdossiers": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "titel": {
            "type": "string"
          },
          "bestanden": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string",
              "format": "uri"
            }
          }
        }
      }
    }
  }
}
